{
  "manifest_version": 3,
  "name": "Quick UC",
  "version": "0.2",
  "description": "Launch frequently used web pages for PUC students",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "180": "images/icon-180.png"
    }
  },
  "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "180": "images/icon-180.png",
      "192": "images/icon-192.png",
      "512": "images/icon-512.png"
  }
}
